{"remainingRequest":"C:\\Users\\lol\\Desktop\\pressserver\\press\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\lol\\Desktop\\pressserver\\press\\src\\views\\Articleview.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"C:\\Users\\lol\\Desktop\\pressserver\\press\\src\\views\\Articleview.vue","mtime":1623186396309},{"path":"C:\\Users\\lol\\Desktop\\pressserver\\press\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\lol\\Desktop\\pressserver\\press\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\lol\\Desktop\\pressserver\\press\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\lol\\Desktop\\pressserver\\press\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\lol\\Desktop\\pressserver\\press\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Articleview.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqJA;AACA;AACA;AACA","file":"Articleview.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n<div class=\"ArticleView mx-auto\">\r\n    <v-row no-gutters>\r\n        <v-col class=\"d-flex my-5\" cols=\"12\">\r\n            <p style=\"cursor:pointer;\" class=\"listSubText\" @click=\"toHome\"><v-icon small>mdi-home</v-icon>Home</p>\r\n            <v-icon small class=\"mx-3\">mdi-chevron-right</v-icon>\r\n            <p style=\"cursor:pointer; font-weight:900;\" class=\"listSubText\" @click=\"toCate\">{{queryTitle}}</p>\r\n        </v-col>\r\n    </v-row>\r\n    <v-row no-gutters>\r\n        <v-col class=\"my-3\" cols=\"12\">\r\n            <p class=\"newsTitleText\">{{title}}</p>\r\n        </v-col>\r\n        <v-col cols=\"auto my-3\">\r\n            <p class=\"listTinyText\"><v-icon small>mdi-account</v-icon>{{author}}</p>\r\n        </v-col>\r\n        <v-divider style=\"height:15px;\" class=\"mx-5 my-3\" vertical></v-divider>\r\n        <v-col cols=\"auto my-3\">\r\n            <p class=\"listTinyText\"><v-icon small>mdi-clock-time-three-outline</v-icon>{{regTime.slice(0,16).replace('T','｜')}}</p>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n        <v-col cols=\"auto\">\r\n            <v-tooltip top>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <v-btn @click=\"urlShare\" v-bind=\"attrs\" v-on=\"on\"><v-icon>mdi-link-variant</v-icon></v-btn>\r\n                </template>\r\n                <span>URL 공유하기</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n    </v-row>\r\n    <v-divider class=\"mt-4\"></v-divider>\r\n    <v-row no-gutters>\r\n        <v-col class=\"mt-12\" cols=\"12\" lg=\"9\">\r\n            <div style=\"width:100%;\">\r\n                <div id=\"htmlViewer\" v-html=\"contents\"></div>\r\n            </div>\r\n        </v-col>\r\n        <v-col v-if=\"!this.$vuetify.breakpoint.mdAndDown\" cols=\"3\" lg=\"3\">\r\n            <v-row no-gutters>\r\n                <v-col cols=\"12\">\r\n                    <p class=\"newsTitleText ml-3\">최신뉴스</p>\r\n                </v-col>\r\n                <v-col cols=\"12\">\r\n                    <Timeline/>\r\n                </v-col>\r\n            </v-row>\r\n        </v-col>\r\n    </v-row>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport Timeline from '@/components/Timeline.vue'\r\nimport VueCookies from 'vue-cookies'\r\nexport default {\r\n    components:{\r\n        Timeline,\r\n    },\r\n    data(){\r\n        return{\r\n            toggle_exclusive:0,\r\n            title:null,\r\n            author:null,\r\n            bNum:null,\r\n            contents:null,\r\n            regTime:'',\r\n        }\r\n    },\r\n    mounted(){\r\n        this.getBoard();\r\n    },\r\n    methods:{\r\n        getBoard(){\r\n            var n = parseInt(this.$route.query.num);\r\n            axios.post('http://localhost:8002/api/board/findone',{\r\n                seq:n\r\n            })\r\n            .then((res)=>{\r\n                this.title = res.data.title;\r\n                this.author = res.data.author;\r\n                this.bNum = res.data.bNum;\r\n                this.contents = res.data.contents;\r\n                this.regTime = res.data.regTime;\r\n                this.addViews();\r\n\r\n            })\r\n        },\r\n        addViews(){\r\n            if(VueCookies.isKey(`views${this.$route.query.num}`)) return;\r\n            else{\r\n                VueCookies.set(`views${this.$route.query.num}`, new Date());\r\n                axios.put('http://localhost:8002/api/board/addviews',{\r\n                    seq:this.$route.query.num\r\n                })\r\n            }\r\n        },\r\n        urlShare(){\r\n            var link = `http://alldayfootball.co.kr/articleView?num=${this.$route.query.num}`;\r\n            const ele = document.createElement('textarea');\r\n            ele.value = link;\r\n            document.body.appendChild(ele);\r\n            ele.select();\r\n            document.execCommand(\"copy\");\r\n            document.body.removeChild(ele);\r\n            alert('URL이 복사되었습니다. \\n Ctrl+V로 붙여넣을 수 있습니다.');\r\n        },\r\n        toHome(){\r\n            location.href=\"/\";\r\n        },\r\n        toCate(){\r\n            switch(this.bNum){\r\n                case 0 :location.href=\"/Articlelist?name=k1\"; break;\r\n                case 1 :location.href=\"/Articlelist?name=k2\"; break;\r\n                case 2 :location.href=\"/Articlelist?name=k3\"; break;\r\n                case 3 :location.href=\"/Articlelist?name=k4\"; break;\r\n                case 4 :location.href=\"/Articlelist?name=k5\"; break;\r\n                case 5 :location.href=\"/Articlelist?name=interview\"; break;\r\n                case 6 :location.href=\"/Articlelist?name=column\"; break;\r\n                case 7 :location.href=\"/Articlelist?name=kresult\"; break;\r\n                default : return 'title'\r\n            }\r\n        },\r\n    },\r\n    computed:{\r\n            queryTitle(){\r\n            switch(this.bNum){\r\n                case 0 :return 'K1 리그';\r\n                case 1 :return 'K2 리그';\r\n                case 2 :return 'K3 리그';\r\n                case 3 :return 'K4 리그';\r\n                case 4 :return 'K5 리그';\r\n                case 5 :return '인터뷰';\r\n                case 6 :return '스포츠 칼럼';\r\n                case 7 :return 'K리그 경기 결과';\r\n                default : return 'title'\r\n            }\r\n        },\r\n    },\r\n    watch:{\r\n        $route(){\r\n            this.getBoard();\r\n        },\r\n    },\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n#htmlViewer *{\r\n  max-width: 100% !important;\r\n  height: auto !important;\r\n}\r\n</style>"]}]}