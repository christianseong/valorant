qBAsBA;AACA,SAAA,SAAA;AACA,GAxBA;AAyBA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,EAAA,GAAA,EAAA;AACA,UAAA,KAAA,QAAA,KAAA,KAAA,MAAA,EAAA,OAAA,IAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,MAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,gBAAA,KAAA,MAAA,CAAA,CAAA,CAAA;AACA,eAAA,IAAA;AAAA,YAAA,EAAA,CAAA,CAAA,CAAA,GAAA,CAAA;AAAA;;AACA,eAAA,IAAA;AAAA,YAAA,EAAA,CAAA,CAAA,CAAA,GAAA,CAAA;AAAA;;AACA,eAAA,IAAA;AAAA,YAAA,EAAA,CAAA,CAAA,CAAA,GAAA,CAAA;AAAA;;AACA,eAAA,IAAA;AAAA,YAAA,EAAA,CAAA,CAAA,CAAA,GAAA,CAAA;AAAA;;AACA,eAAA,IAAA;AAAA,YAAA,EAAA,CAAA,CAAA,CAAA,GAAA,CAAA;AAAA;;AACA,eAAA,KAAA;AAAA,YAAA,EAAA,CAAA,CAAA,CAAA,GAAA,CAAA;AAAA;;AACA,eAAA,OAAA;AAAA,YAAA,EAAA,CAAA,CAAA,CAAA,GAAA,CAAA;AAAA;;AACA,eAAA,OAAA;AAAA,YAAA,EAAA,CAAA,CAAA,CAAA,GAAA,CAAA;AAAA;AARA;AAUA;;AACA,aAAA,EAAA;AACA,KAjBA;AAkBA,IAAA,OAlBA,qBAkBA;AACA,UAAA,GAAA,GAAA,QAAA,CAAA,KAAA,OAAA,GAAA,EAAA,CAAA;AACA,UAAA,GAAA,GAAA,KAAA,OAAA,GAAA,EAAA;AACA,UAAA,GAAA,KAAA,CAAA,EAAA,OAAA,CAAA;AACA,UAAA,GAAA,GAAA,CAAA,IAAA,GAAA,KAAA,CAAA,EAAA,OAAA,GAAA;AACA,aAAA,GAAA,GAAA,CAAA;AACA;AAxBA,GAzBA;AAmDA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,GADA,EACA;AACA,MAAA,QAAA,CAAA,IAAA,6BAAA,GAAA;AACA,KAHA;AAIA,IAAA,WAJA,yBAIA;AACA,WAAA,SAAA;AACA,KANA;AAOA,IAAA,SAPA,uBAOA;AAAA;;AACA,qBAAA,IAAA,CAAA,iDAAA,EAAA;AACA,QAAA,IAAA,EAAA,KAAA,UADA;AAEA,QAAA,KAAA,EAAA,EAFA;AAGA,QAAA,IAAA,EAAA,KAAA,IAHA;AAIA,QAAA,IAAA,EAAA,KAAA;AAJA,OAAA,EAMA,IANA,CAMA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,SAAA;AACA,OATA;AAUA;AAlBA,GAnDA;AAuEA,EAAA,KAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,SAAA;AACA,KAHA;AAIA,IAAA,MAJA,oBAIA;AACA,WAAA,SAAA;AACA;AANA;AAvEA,C","sourcesContent":["<template>\r\n<div id=\"Article\" class=\"Article\">\r\n    <v-row class=\"my-5\">\r\n        <v-col cols=\"12\">\r\n            <p class=\"mainText\">기사관리</p>\r\n        </v-col>\r\n    </v-row>\r\n    <v-row>\r\n        <v-col cols=\"12\">\r\n            <v-combobox\r\n            color=\"green\"\r\n            v-model=\"select\"\r\n            :items=\"category\"\r\n            label=\"카테고리\"\r\n            solo\r\n            multiple\r\n            chips\r\n            >\r\n                <template v-slot:item=\"{ item }\">\r\n                    <v-chip\r\n                    color=\"green\"\r\n                    dark\r\n                    label\r\n                    small\r\n                    >\r\n                    {{item}}\r\n                    </v-chip>\r\n                </template>\r\n\r\n                <template v-slot:selection=\"{item, attrs, select }\">\r\n\r\n                    <v-chip\r\n                    v-bind=\"attrs\"\r\n                    color=\"rgba(88,222,88,.5)\"\r\n                    :input-value=\"select\"\r\n                    label\r\n                    small\r\n                    >\r\n                        <p style=\"color:green;\" class=\"headerText\">{{item}}</p>\r\n                    </v-chip>\r\n                </template>\r\n                \r\n            </v-combobox>\r\n        </v-col>\r\n    </v-row>\r\n    <v-row>\r\n        <v-col class=\"d-flex align-center\" cols=\"12\">\r\n        <v-text-field @keypress.enter=\"clickSearch\" hide-details v-model=\"search\" label=\"검색\" solo></v-text-field>\r\n        <v-btn icon style=\"height:90%;\" class=\"mx-3 px-2\" @click=\"clickSearch\" color=\"#0C9045\" ><v-icon>mdi-magnify</v-icon></v-btn>\r\n        </v-col>\r\n    </v-row>\r\n\r\n    <div v-if=\"!this.$vuetify.breakpoint.mdAndDown\">\r\n    <v-row style=\"text-align:center;\" class=\"mt-5\" no-gutters>\r\n        <v-col class=\"py-3\" style=\"background-color:#0C9045;\" cols=\"1\">\r\n            <p style=\"color:white; border-right:1px solid white;\" class=\"sliderTitleText\">No</p>\r\n        </v-col>\r\n        <v-col class=\"py-3\" style=\"background-color:#0C9045;\" cols=\"1\">\r\n            <p style=\"color:white; border-right:1px solid white;\" class=\"sliderTitleText\">분류</p>\r\n        </v-col>\r\n        <v-col class=\"py-3\" style=\"background-color:#0C9045;\" cols=\"5\">\r\n            <p style=\"color:white; border-right:1px solid white;\" class=\"sliderTitleText\">제목</p>\r\n        </v-col>\r\n        <v-col class=\"py-3\" style=\"background-color:#0C9045;\" cols=\"2\">\r\n            <p style=\"color:white; border-right:1px solid white;\" class=\"sliderTitleText\">작성자</p>\r\n        </v-col>\r\n        <v-col class=\"py-3\" style=\"background-color:#0C9045;\" cols=\"1\">\r\n            <p style=\"color:white; border-right:1px solid white;\" class=\"sliderTitleText\">조회수</p>\r\n        </v-col>\r\n        <v-col class=\"py-3\" style=\"background-color:#0C9045;\" cols=\"2\">\r\n            <p style=\"color:white;\" class=\"sliderTitleText\">날짜</p>\r\n        </v-col>\r\n    </v-row>\r\n\r\n    <v-row style=\"text-align:center;border-bottom:1px solid rgba(0,0,0,.2);\" no-gutters v-for=\"(i,index) in boardResult\" :key=\"index\">\r\n        <v-col class=\"py-2\" cols=\"1\">\r\n            <p style=\"overflow:hidden; border-right:1px solid rgba(0,0,0,.2);;\" class=\"sliderTitleText\">{{i.seq}}</p>\r\n        </v-col>\r\n        <v-col class=\"py-2\" cols=\"1\">\r\n            <p style=\"overflow:hidden; border-right:1px solid rgba(0,0,0,.2);;\" class=\"sliderTitleText\">{{category[i.bNum]}}</p>\r\n        </v-col>\r\n        <v-col class=\"py-2\" cols=\"5\">\r\n            <p @click=\"clickTitle(i.seq)\" style=\"overflow:hidden; border-right:1px solid rgba(0,0,0,.2);; cursor:pointer;\" class=\"newsSubText\">{{i.title}}</p>\r\n        </v-col>\r\n        <v-col class=\"py-2\" cols=\"2\">\r\n            <p style=\"border-right:1px solid rgba(0,0,0,.25);;\" class=\"sliderTitleText\">{{i.author}}</p>\r\n        </v-col>\r\n        <v-col class=\"py-2\" cols=\"1\">\r\n            <p style=\"overflow:hidden; border-right:1px solid rgba(0,0,0,.2);;\" class=\"sliderTitleText\">{{i.views}}</p>\r\n        </v-col>\r\n        <v-col class=\"py-2\" cols=\"2\">\r\n            <p class=\"sliderTitleText\">{{i.regTime.slice(0,10)}}</p>\r\n        </v-col>\r\n    </v-row>\r\n    </div>\r\n\r\n    <div v-if=\"this.$vuetify.breakpoint.mdAndDown\">\r\n    <v-row style=\"text-align:center;\" class=\"mt-5\" no-gutters>\r\n        <v-col class=\"py-3\" style=\"background-color:#0C9045;\" cols=\"6\">\r\n            <p style=\"color:white; border-right:1px solid white;\" class=\"sliderTitleText\">제목</p>\r\n        </v-col>\r\n        <v-col class=\"py-3\" style=\"background-color:#0C9045;\" cols=\"3\">\r\n            <p style=\"color:white; border-right:1px solid white;\" class=\"sliderTitleText\">작성자</p>\r\n        </v-col>\r\n        <v-col class=\"py-3\" style=\"background-color:#0C9045;\" cols=\"3\">\r\n            <p style=\"color:white;\" class=\"sliderTitleText\">날짜</p>\r\n        </v-col>\r\n    </v-row>\r\n\r\n    <v-row style=\"text-align:center;border-bottom:1px solid rgba(0,0,0,.2);\" no-gutters v-for=\"(i,index) in boardResult\" :key=\"index\">\r\n        <v-col class=\"py-2\" cols=\"6\">\r\n            <p @click=\"clickTitle(i.seq)\" style=\"overflow:hidden; border-right:1px solid rgba(0,0,0,.2);; cursor:pointer;\" class=\"sliderTitleText\">{{i.title}}</p>\r\n        </v-col>\r\n        <v-col class=\"py-2\" cols=\"3\">\r\n            <p style=\"border-right:1px solid rgba(0,0,0,.25);;\" class=\"sliderTitleText\">{{i.author}}</p>\r\n        </v-col>\r\n        <v-col class=\"py-2\" cols=\"3\">\r\n            <p class=\"sliderTitleText\">{{i.regTime.slice(0,10)}}</p>\r\n        </v-col>\r\n    </v-row>\r\n    </div>\r\n\r\n    <v-row>\r\n        <v-col class=\"d-flex justify-end mt-6 mr-3\" cols=\"12\">\r\n            <v-btn color=\"#0C9045\" to=\"/admin/write\"><v-icon color=\"white\">mdi-pencil-plus</v-icon><p class=\"subText\" style=\"color:white;\">글쓰기</p></v-btn>\r\n        </v-col>\r\n    </v-row>\r\n    <v-row>\r\n        <v-col cols=\"12\">\r\n            <div class=\"text-center\">\r\n                <v-container>\r\n                <v-row justify=\"center\">\r\n                    <v-col cols=\"8\">\r\n                    <v-container class=\"max-width\">\r\n                        <v-pagination\r\n                        v-model=\"page\"\r\n                        class=\"my-4\"\r\n                        color=\"#0C9045\"\r\n                        :length=\"pLength\"\r\n                        ></v-pagination>\r\n                    </v-container>\r\n                    </v-col>\r\n                </v-row>\r\n                </v-container>\r\n            </div>\r\n        </v-col>\r\n    </v-row>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\naxios.defaults.headers['Pragma'] = 'no-cache';\r\nexport default {\r\n    data(){\r\n        return{\r\n            search:'',\r\n            titleCheck: true,\r\n            contentCheck: true,\r\n            bLength:0,\r\n            boardResult:[],\r\n            page:1,\r\n            category:[\r\n                'k1',\r\n                'k2',\r\n                'k3',\r\n                'k4',\r\n                'k5',\r\n                '인터뷰',\r\n                '스포츠칼럼',\r\n                'K리그결과',\r\n            ],\r\n            select:['k1','k2','k3','k4','k5','인터뷰','스포츠칼럼','K리그결과'],\r\n        }\r\n    },\r\n    created(){\r\n        this.takeBoard();\r\n    },\r\n    computed:{\r\n        selectCode(){\r\n            var sc=[];\r\n            if(this.category===this.select) return null;\r\n            for(var i =0; i<this.select.length; i++){\r\n                switch(this.select[i]){\r\n                    case 'k1': sc[i] = 0; break;\r\n                    case 'k2': sc[i] = 1; break;\r\n                    case 'k3': sc[i] = 2; break;\r\n                    case 'k4': sc[i] = 3; break;\r\n                    case 'k5': sc[i] = 4; break;\r\n                    case '인터뷰': sc[i] = 5; break;\r\n                    case '스포츠칼럼': sc[i] = 6; break;\r\n                    case 'K리그결과': sc[i] = 7; break;\r\n                }\r\n            }\r\n            return sc;\r\n        },\r\n        pLength(){\r\n            var quo = parseInt(this.bLength/10);\r\n            var rem = this.bLength % 10 ;\r\n            if(quo===0) return 1;\r\n            if(quo>0&&rem===0) return quo;\r\n            return quo + 1 ;\r\n        },\r\n    },\r\n    methods:{\r\n        clickTitle(num){\r\n            location.href=`/admin/edit?num=${num}`;\r\n        },\r\n        clickSearch(){\r\n            this.takeBoard();\r\n        },\r\n        takeBoard(){\r\n            axios.post('http://alldayfootball.co.kr/api/board/takeboard',{\r\n                bNum: this.selectCode,\r\n                limit: 10,\r\n                page: this.page,\r\n                word:this.search\r\n            })\r\n            .then((res=>{\r\n                this.boardResult = res.data.docs;\r\n                this.bLength = res.data.totalDocs;\r\n            }))\r\n        },\r\n    },\r\n    watch:{\r\n        page(){\r\n            this.takeBoard();\r\n        },\r\n        select(){\r\n            this.takeBoard();\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n.Article .v-pagination__navigation{\r\n    height: 25px !important;\r\n    width: 25px !important;\r\n}\r\n.Article .v-pagination__item{\r\n    min-width: 25px !important;\r\n    height: 25px !important;\r\n}\r\n.v-application .primary--text{\r\n    color:green !important\r\n}\r\n</style>\r\n\r\n<style scoped>\r\n@import url(\"../../assets/css/unify.css\");\r\n</style>"],"sourceRoot":"src/components/Admin"}]}