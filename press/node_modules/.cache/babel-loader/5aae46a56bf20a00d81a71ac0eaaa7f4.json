A,MAAA,KAAA,CAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,KANA;AAQA,GAtCA;AAuCA,EAAA,IAvCA,kBAuCA;AACA,WAAA;AACA,MAAA,MAAA,EAAA,IADA;AAEA,MAAA,EAAA,EAAA,IAFA;AAGA,MAAA,MAAA,EAAA,MAHA;AAIA,MAAA,KAAA,EAAA,CACA,MADA,EAEA,MAFA,EAGA,SAHA;AAJA,KAAA;AAUA,GAlDA;AAmDA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;;AACA,qBAAA,IAAA,CAAA,sDAAA,EAAA;AACA,QAAA,EAAA,EAAA,KAAA;AADA,OAAA,EAGA,IAHA,CAGA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,EAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,OANA;AAOA;AATA,GAnDA;AA8DA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,cAAA,KAAA,MAAA;AACA,aAAA,MAAA;AAAA,iBAAA,0BAAA;;AACA,aAAA,MAAA;AAAA,iBAAA,0BAAA;;AACA,aAAA,SAAA;AAAA,iBAAA,0BAAA;;AACA;AAAA,iBAAA,0BAAA;AAJA;AAMA;AARA,GA9DA;AAwEA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,qBAAA,GAAA,CAAA,mDAAA,EAAA;AACA,QAAA,EAAA,EAAA,KAAA,EADA;AAEA,QAAA,QAAA,EAAA,KAAA,MAAA,CAAA;AAFA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,IAAA,GAAA,mBAAA;AACA,OANA;AAOA;AATA;AAxEA,C","sourcesContent":["<template>\r\n<div class=\"ConfigEdit\">\r\n    <v-row class=\"mt-5\" no-gutters>\r\n        <v-col cols=\"12\">\r\n            <v-combobox solo-inverted chips v-model=\"select\" :items=\"items\" ></v-combobox>\r\n        </v-col>\r\n        <v-col style=\"border-bottom:1px solid grey;\" cols=\"12\">\r\n            <div id=\"editor2\"></div>\r\n        </v-col>\r\n        <v-col class=\"d-flex justify-center\" cols=\"12\">\r\n            <v-btn class=\"ma-3\" @click=\"clickSubmit\" color=\"green\"><v-icon color=\"white\">mdi-upload</v-icon><p class=\"subText\" style=\"color:white;\">수정하기</p></v-btn>\r\n        </v-col>\r\n    </v-row>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\naxios.defaults.headers['Pragma'] = 'no-cache';\r\nimport { Jodit } from 'jodit'\r\nimport 'jodit/build/jodit.min.css'\r\nexport default {\r\n    mounted() {\r\n         var uploadOptions = {\r\n             height:'70vh',\r\n                enableDragAndDropFileToEditor: true,\r\n                uploader: {\r\n                    url: 'http://alldayfootball.co.kr/api/storage/test',\r\n                    filesVariableName: function () {\r\n                        return \"files\";\r\n                    },\r\n                    isSuccess: function (resp) {\r\n                        return resp;\r\n                    },\r\n                    process: function (resp) {\r\n                        return {\r\n                            // files: resp.data.files,\r\n                            path: resp.path,\r\n                            // baseurl: resp.data.baseurl,\r\n                            // error: resp.data.error,\r\n                            // message: resp.message\r\n                        }\r\n                    },\r\n                    defaultHandlerSuccess: function (data) {\r\n                        this.selection.insertImage(data.path);\r\n                    },\r\n                }\r\n         }\r\n        this.editor = new Jodit('#editor2',uploadOptions)\r\n        this.editor.value = \"\";\r\n            // console.log(n);\r\n            axios.post('http://alldayfootball.co.kr/api/settingboard/findone',{\r\n                id:this.selectId\r\n            })\r\n            .then((res)=>{\r\n                this.id = res.data._id;\r\n                this.editor.value = res.data.contents;\r\n            })\r\n\r\n    },\r\n    data(){\r\n        return{\r\n            editor:null,\r\n            id:null,\r\n            select: '매체소개',\r\n            items: [\r\n                '매체소개',\r\n                '불편신고',\r\n                '약관 및 정책',\r\n            ],\r\n        }\r\n    },\r\n    watch:{\r\n        select(){\r\n            axios.post('http://alldayfootball.co.kr/api/settingboard/findone',{\r\n                id:this.selectId\r\n            })\r\n            .then((res)=>{\r\n                this.id = res.data._id;\r\n                this.editor.value = res.data.contents;\r\n            })\r\n        },\r\n    },\r\n    computed:{\r\n        selectId(){\r\n            switch(this.select){\r\n                case '매체소개' : return \"60ce99f5b7062e4c6c092cb3\";\r\n                case '불편신고' : return \"60ce9a39b7062e4c6c092cb4\";\r\n                case '약관 및 정책' : return \"60ce9a4fb7062e4c6c092cb5\";\r\n                default: return \"60ce99f5b7062e4c6c092cb3\";\r\n            }\r\n        },\r\n    },\r\n    methods:{\r\n        clickSubmit(){\r\n            axios.put('http://alldayfootball.co.kr/api/settingboard/edit',{\r\n                id: this.id,\r\n                contents: this.editor.value\r\n            })\r\n            .then(()=>{\r\n                window.location.href = \"/admin/configtext\"\r\n            })\r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n.ConfigEdit .v-text-field.v-text-field--solo:not(.v-text-field--solo-flat) > .v-input__control > .v-input__slot{\r\n    box-shadow: none;\r\n}\r\n</style>\r\n\r\n<style scoped>\r\n@import url(\"../../assets/css/unify.css\");\r\n</style>"],"sourceRoot":"src/components/Admin"}]}