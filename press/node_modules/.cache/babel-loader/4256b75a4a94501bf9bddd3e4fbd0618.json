A;AAAA,eAAA,SAAA,CAAA,CAAA;AAAA;;AACA,aAAA,IAAA;AAAA,eAAA,SAAA,CAAA,CAAA;AAAA;;AACA,aAAA,IAAA;AAAA,eAAA,SAAA,CAAA,CAAA;AAAA;;AACA,aAAA,IAAA;AAAA,eAAA,SAAA,CAAA,CAAA;AAAA;;AACA,aAAA,IAAA;AAAA,eAAA,SAAA,CAAA,CAAA;AAAA;;AACA,aAAA,WAAA;AAAA,eAAA,SAAA,CAAA,CAAA;AAAA;;AACA,aAAA,QAAA;AAAA,eAAA,SAAA,CAAA,CAAA;AAAA;;AACA,aAAA,SAAA;AAAA,eAAA,SAAA,CAAA,CAAA;AAAA;;AACA;AAAA,eAAA,SAAA,CAAA,CAAA;AAAA;AATA;AAWA;AAbA;AA7EA,CAAA","sourcesContent":["<template>\r\n<div class=\"articlelist\">\r\n    <v-row no-gutters>\r\n        <v-col cols=\"12\" lg=\"9\">\r\n            <v-row no-gutters>\r\n                <v-col cols=\"auto\">\r\n                    <p class=\"titleSubText\">{{queryTitle}} <span class=\"listSubText\">({{this.boards.length}}건)</span></p>\r\n                </v-col>\r\n                <v-spacer></v-spacer>\r\n                <v-col class=\"d-flex align-end\" cols=\"auto\">\r\n                    <v-btn-toggle v-model=\"toggle_exclusive\">\r\n                        <v-btn small>\r\n                            <v-icon small>mdi-image-text</v-icon>\r\n                            <p>요약형</p>\r\n                        </v-btn>\r\n                        <v-btn small>\r\n                            <v-icon small>mdi-format-align-justify</v-icon>\r\n                            <p>제목형</p>\r\n                        </v-btn>\r\n                        <v-btn small>\r\n                            <v-icon small>mdi-format-underline</v-icon>\r\n                            <p>포토형</p>\r\n                        </v-btn>\r\n                    </v-btn-toggle>\r\n                </v-col>\r\n            </v-row>\r\n            <v-divider style=\"border-bottom:2px solid rgba(0, 0, 0, 0.3);\"></v-divider>\r\n            <v-row no-gutters>\r\n                <v-col class=\"my-3 pa-3\" style=\"border-top:1px solid rgba(0,0,0,.1); border-bottom:1px solid rgba(0,0,0,.1)\" v-for=\"i in boardPage\" :key=\"i\" cols=\"12\">\r\n                    <div style=\"width:100%; height:150px;\">\r\n                        <v-row no-gutters>\r\n                            <v-col cols=\"3\">\r\n                                <v-card @click=\"goToView(i.seq)\" width=\"100%\" height=\"150\"><v-img height=\"100%\" width=\"100%\" :src=\"i.thumb\"></v-img></v-card>\r\n                            </v-col>\r\n                            <v-spacer></v-spacer>\r\n                            <v-col cols=\"8\">\r\n                                <v-row no-gutters>\r\n                                    <v-col class=\"my-3\" cols=\"12\">\r\n                                        <p @click=\"goToView(i.seq)\" style=\"cursor:pointer\" class=\"newsTitleText\">{{i.title}}</p>\r\n                                    </v-col>\r\n                                </v-row>\r\n                                <v-row no-gutters>\r\n                                    <v-col cols=\"12\" class=\"my-3\">\r\n                                        <p @click=\"goToView(i.seq)\" style=\"cursor:pointer\" class=\"listSubText\">{{i.pretext}}</p>\r\n                                    </v-col>\r\n                                </v-row>\r\n                                <v-row no-gutters>\r\n                                    <v-col cols=\"12\">\r\n                                        <p class=\"listTinyText\">{{i.author}} ｜ {{i.regTime.slice(0,16).replace('T','｜')}}</p>\r\n                                    </v-col>\r\n                                </v-row>\r\n                            </v-col>\r\n                        </v-row>\r\n                    </div>\r\n                </v-col>\r\n            </v-row>\r\n            <v-row no-gutters>\r\n                    <v-col cols=\"12\">\r\n                        <div class=\"text-center\">\r\n                            <v-container>\r\n                            <v-row justify=\"center\">\r\n                                <v-col cols=\"8\">\r\n                                <v-container class=\"max-width\">\r\n                                    <v-pagination\r\n                                    v-model=\"page\"\r\n                                    class=\"my-4\"\r\n                                    :length=\"pLength\"\r\n                                    ></v-pagination>\r\n                                </v-container>\r\n                                </v-col>\r\n                            </v-row>\r\n                            </v-container>\r\n                        </div>\r\n                    </v-col>\r\n            </v-row>\r\n        </v-col>\r\n        <v-col v-if=\"!this.$vuetify.breakpoint.mdAndDown\" cols=\"3\">\r\n            <v-row no-gutters style=\"height:200px;\">\r\n                광고3\r\n            </v-row>\r\n            <v-divider style=\"width:90%; border-bottom:2px solid rgba(0, 0, 0, 0.3);\"></v-divider>\r\n            <v-row no-gutters>\r\n                <v-col cols=\"12\">\r\n                    <p>최신뉴스</p>\r\n                <v-divider></v-divider>\r\n                </v-col>\r\n                <v-col cols=\"12\">\r\n                    <div style=\"background-color:red; width:100%; height:350px;\"></div>\r\n                </v-col>\r\n            </v-row>\r\n            <v-row no-gutters>\r\n                <v-col cols=\"12\">\r\n                    <p>인기뉴스</p>\r\n                <v-divider></v-divider>\r\n                </v-col>\r\n                <v-col cols=\"12\">\r\n                    <div style=\"background-color:red; width:100%; height:350px;\"></div>\r\n                </v-col>\r\n            </v-row>\r\n            <v-row no-gutters>\r\n                <v-col cols=\"12\">\r\n                    <p>뉴스</p>\r\n                <v-divider></v-divider>\r\n                </v-col>\r\n                <v-col cols=\"12\">\r\n                    <div style=\"background-color:red; width:100%; height:350px;\"></div>\r\n                </v-col>\r\n            </v-row>\r\n        </v-col>\r\n    </v-row>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n    data(){\r\n        return{\r\n            boards:[],\r\n            toggle_exclusive:0,\r\n            page:1,\r\n        }\r\n    },\r\n    mounted(){\r\n        switch(this.$route.query.name){\r\n        case 'k1' : this.getBoards(0); break;\r\n        case 'k2' : this.getBoards(1); break;\r\n        case 'k3' : this.getBoards(2); break;\r\n        case 'k4' : this.getBoards(3); break;\r\n        case 'k5' : this.getBoards(4); break;\r\n        case 'interview' : this.getBoards(5); break;\r\n        case 'column' : this.getBoards(6); break;\r\n        case 'kresult' : this.getBoards(7); break;\r\n        default : this.getBoards(0); break;\r\n        }\r\n    },\r\n    methods:{\r\n        getBoards(Num){\r\n            axios.post('http://localhost:8002/api/board/findcate',{\r\n                bNum:Num,\r\n            })\r\n            .then((res)=>{\r\n                this.boards=res.data;\r\n                this.findThumb();\r\n                this.findPretext();\r\n            })\r\n        },\r\n        findThumb(){\r\n            for(var i = 0; i<this.boards.length; i++){\r\n                if(this.boards[i].contents.includes('<img')){\r\n                var tagIndex = this.boards[i].contents.indexOf('<img');\r\n                var tagSrcIndex = this.boards[i].contents.indexOf('src=\"',tagIndex+4);\r\n                var tagEndIndex = this.boards[i].contents.indexOf('\"',tagSrcIndex+5);\r\n                this.boards[i].thumb = this.boards[i].contents.slice(tagSrcIndex+5,tagEndIndex);\r\n                }\r\n            }\r\n        },\r\n        findPretext(){\r\n            for(var i = 0; i<this.boards.length; i++){\r\n                this.boards[i].pretext = this.boards[i].contents.replace(/(<([^>]+)>|&nbsp;)/ig,\" \").slice(0,100)+('...');\r\n            }\r\n        },\r\n        goToView(num){\r\n            location.href=`/articleView?num=${num}`;\r\n        },\r\n    },\r\n    computed:{\r\n        queryTitle(){\r\n            switch(this.$route.query.name){\r\n                case 'k1' :return 'K1 리그';\r\n                case 'k2' :return 'K2 리그';\r\n                case 'k3' :return 'K3 리그';\r\n                case 'k4' :return 'K4 리그';\r\n                case 'k5' :return 'K5 리그';\r\n                case 'interview' :return '인터뷰';\r\n                case 'column' :return '스포츠 칼럼';\r\n                case 'kresult' :return 'K리그 경기 결과';\r\n                default : return 'title'\r\n            }\r\n        },\r\n        pLength(){\r\n            var bLength = this.boards.length;\r\n            var quo = parseInt(bLength/10);\r\n            var rem = bLength % 10 ;\r\n            if(quo===0) return 1;\r\n            if(quo>0&&rem===0) return quo;\r\n            return quo + 1 ;\r\n        },\r\n        boardPage(){\r\n            return this.boards.slice((this.page-1)*10,this.page*10);\r\n        },\r\n    },\r\n    watch:{\r\n        $route(){\r\n            switch(this.$route.query.name){\r\n                case 'k1' : this.getBoards(0); break;\r\n                case 'k2' : this.getBoards(1); break;\r\n                case 'k3' : this.getBoards(2); break;\r\n                case 'k4' : this.getBoards(3); break;\r\n                case 'k5' : this.getBoards(4); break;\r\n                case 'interview' : this.getBoards(5); break;\r\n                case 'column' : this.getBoards(6); break;\r\n                case 'kresult' : this.getBoards(7); break;\r\n                default : this.getBoards(0); break;\r\n            }\r\n        },\r\n    },\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import url(\"../assets/css/unify.css\");\r\n\r\n</style>"],"sourceRoot":"src/views"}]}